{
  "services": [
    {
      "name": "frontend",
      "path": ".",
      "builder": "vite",
      "buildCommand": "npm run build",
      "startCommand": "npm run preview",
      "port": 4173
    },
    {
      "name": "backend",
      "path": "server",
      "builder": "nodejs",
      "buildCommand": "npm install",
      "startCommand": "node index.js",
      "port": 3002
    }
  ]
}
